    "def calculate_angle(p1,p2,p3):\n",
    "    # creating the angle p1p2p3\n",
    "    vec1 = p1 - p2\n",
    "    vec2 = p3 - p2\n",
    "    cos_theta = np.dot(vec1,vec2)/ (np.linalg.norm(vec1) * np.linalg.norm(vec2))\n",
    "    theta = np.arccos(cos_theta)\n",
    "    return np.degrees(theta)\n",
    "    "
   ]
